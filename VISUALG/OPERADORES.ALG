Algoritmo "Operadores"

Var
   N1, N2, N3, M : REAL


Inicio
   ESCREVA ("INSIRA SUA NOTA NOTA 1: ")
   LEIA  (N1)
   ESCREVA ("INSIRA SUA NOTA NOTA 2: ")
   LEIA (N2)
   ESCREVA ("INSIRA SUA NOTA NOTA 3: ")
   LEIA (N3)
   M <- (N1 + N2 + N3)/3
   ESCREVA ("SUA MEDIA É ", M)


Fimalgoritmo